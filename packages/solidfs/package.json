{
  "name": "solidfs",
  "publisher": "inrupt",
  "displayName": "solidFS",
  "description": "View your Solid Pod as a local File System",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/vscode-extension-solidfs.git"
  },
  "bugs": {
    "email": "support@inrupt.com",
    "url": "https://github.com/inrupt/vscode-extension-solidfs/issues"
  },
  "homepage": "https://github.com/microsoft/vscode-wordcount/blob/main/solidfs/README.md",
  "galleryBanner": {
    "color": "#7c4dff",
    "theme": "dark"
  },
  "icon": "image.png",
  "extensionKind": [
    "workspace"
  ],
  "extensionDependencies": [
    "inrupt.solidauth"
  ],
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:solidfs.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "solidfs.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package:webpack",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package:webpack": "webpack --mode production --devtool hidden-source-map",
    "package:vsce": "vsce package --no-dependencies",
    "package": "npm run package:vsce && npm run package:webpack",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@comunica/query-sparql-solid": "^2.3.0",
    "@inrupt/solid-client": "^1.23.3",
    "@inrupt/solid-client-authn-node": "^1.12.2",
    "@rdfjs/types": "^1.1.0",
    "http-link-header": "^1.0.5",
    "n3": "^1.16.2"
  }
}
